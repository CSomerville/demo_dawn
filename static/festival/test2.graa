StateSpace Test {
    shape: Oval | Square | Rectangle | Circle;
    color: Yellow | Fuschia | Lime;
    texture: Bumpy | Smooth;
}

TransitionOn Test {
    current {
        shape: Oval;
    }
    next {
        shape: Square;
    }
}

TransitionOn Test {
    current {
        shape: Oval;
    }
    next {
        shape: Circle;
    }
}

TransitionOn Test {
    current {
        shape: Square | Rectangle;
